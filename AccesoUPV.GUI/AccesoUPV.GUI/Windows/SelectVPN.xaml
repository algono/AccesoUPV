<Window x:Class="AccesoUPV.GUI.SelectVPN"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccesoUPV.GUI"
        mc:Ignorable="d"
        Title="SelectVPN" FontSize="16" Height="350" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="20" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="20" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="*" />
            <RowDefinition Height="20" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="20" />
        </Grid.RowDefinitions>

        <ListBox x:Name="VPNList" Grid.Column="1" Grid.Row="1" Grid.ColumnSpan="3" />
        <Button x:Name="SelectButton" Grid.Column="1" Grid.Row="3" Content="Select" Click="SelectButton_Click" />
        <Button x:Name="CancelButton" Grid.Column="3" Grid.Row="3" Content="Cancel" Click="CancelButton_Click" />
    </Grid>
</Window>
