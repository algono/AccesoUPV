<Page x:Class="AccesoUPV.GUI.Windows.Main.Pages.Start"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AccesoUPV.GUI.Windows.Main.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Start">

    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="20" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="20" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="20" />
            <RowDefinition Height="*" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="20" />
            <RowDefinition Height="auto" />
            <RowDefinition Height="*" />
            <RowDefinition Height="20" />
        </Grid.RowDefinitions>

        <Button x:Name="ConnectButton" Grid.Column="1" Grid.Row="2" FontSize="36" Content="Conexión" VerticalAlignment="Center" HorizontalAlignment="Center" Click="ConnectButton_Click" />
        <Button x:Name="PrefsButton" Grid.Column="1" Grid.Row="4" FontSize="36" Content="Ajustes" VerticalAlignment="Center" HorizontalAlignment="Center" Click="PrefsButton_Click" />
    </Grid>
</Page>
